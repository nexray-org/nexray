    "dev": "next dev -p 1421",
    "start": "next start",
    "test": "true",
    "build": "echo \"Skip Next.js build\"",
    "deploy": "next build",
    "prebuild": "shx rm -f tsconfig.package.tsbuildinfo",
    "clean": "shx rm -rf build pnpm-lock.yaml node_modules tsconfig.package.tsbuildinfo",
    "env:start": "next dev -p 1421",
    "env:package-old": "next build && shx rm -rf ../app-slim && shx rm -rf ./.next/standalone/node_modules && shx cp -R ./.next/standalone ../app-slim && shx cp -R ./.next/static ../app-slim/.next/static && cd ../app-slim && pnpm i",
