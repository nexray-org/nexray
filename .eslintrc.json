{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off"
    }
  }